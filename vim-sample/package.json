{
	"name": "vim",
	"displayName": "vim",
	"version": "0.0.1",
	"private": false,
	"description": "vim sample extension",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"keybindings": [
			{
				"command": "vim.goToNormalMode",
				"key": "escape",
				"when": "editorTextFocus && !vim.inNormalMode"
			},
			{
				"command": "vim.clearInput",
				"key": "escape",
				"when": "editorTextFocus && vim.hasInput"
			},
			{
				"command": "redo",
				"key": "ctrl+r",
				"mac": "cmd+r",
				"when": "editorTextFocus"
			},
			{
				"command": "e",
				"key": "ctrl+e",
				"when": "editorTextFocus && vim.inNormalMode"
			},
			{
				"command": "d",
				"key": "ctrl+d",
				"when": "editorTextFocus && vim.inNormalMode"
			},
			{
				"command": "f",
				"key": "ctrl+f",
				"when": "editorTextFocus && vim.inNormalMode"
			},
			{
				"command": "y",
				"key": "ctrl+y",
				"when": "editorTextFocus && vim.inNormalMode"
			},
			{
				"command": "u",
				"key": "ctrl+u",
				"when": "editorTextFocus && vim.inNormalMode"
			},
			{
				"command": "b",
				"key": "ctrl+b",
				"when": "editorTextFocus && vim.inNormalMode"
			}
		]
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "^18"
	},
	"publishConfig": {
		"access": "public"
	}
}
