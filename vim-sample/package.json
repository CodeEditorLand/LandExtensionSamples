{
  "name": "vim",
  "displayName": "vim",
  "description": "vim sample extension",
  "version": "0.0.1",
  "publisher": "playform",
  "private": "false",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "vim.goToNormalMode",
        "key": "escape",
        "when": "editorTextFocus && !vim.inNormalMode"
      },
      {
        "command": "vim.clearInput",
        "key": "escape",
        "when": "editorTextFocus && vim.hasInput"
      },
      {
        "command": "redo",
        "key": "ctrl+r",
        "mac": "cmd+r",
        "when": "editorTextFocus"
      },
      {
        "command": "e",
        "key": "ctrl+e",
        "when": "editorTextFocus && vim.inNormalMode"
      },
      {
        "command": "d",
        "key": "ctrl+d",
        "when": "editorTextFocus && vim.inNormalMode"
      },
      {
        "command": "f",
        "key": "ctrl+f",
        "when": "editorTextFocus && vim.inNormalMode"
      },
      {
        "command": "y",
        "key": "ctrl+y",
        "when": "editorTextFocus && vim.inNormalMode"
      },
      {
        "command": "u",
        "key": "ctrl+u",
        "when": "editorTextFocus && vim.inNormalMode"
      },
      {
        "command": "b",
        "key": "ctrl+b",
        "when": "editorTextFocus && vim.inNormalMode"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "devDependencies": {
    "@types/node": "20.9.4",
    "@types/vscode": "1.84.2",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "eslint": "8.54.0",
    "typescript": "5.3.2",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  }
}
