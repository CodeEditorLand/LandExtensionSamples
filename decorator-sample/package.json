{
  "name": "decorator-sample",
  "description": "Sample for the decorator API",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "license": "MIT",
  "engines": {
    "vscode": "1.73.0"
  },
  "repository": {
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git",
    "type": "git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "colors": [
      {
        "id": "myextension.largeNumberBackground",
        "description": "Background decoration color for large numbers",
        "defaults": {
          "dark": "#FF000055",
          "light": "#FF000055",
          "highContrast": "#FF000055"
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts"
  },
  "devDependencies": {
    "@types/node": "20.8.9",
    "@types/vscode": "1.83.1",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.52.0",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  }
}
