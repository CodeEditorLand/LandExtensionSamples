{
	"name": "helloworld-web-sample",
	"displayName": "helloworld-web-sample",
	"version": "0.0.1",
	"private": false,
	"description": "HelloWorld example for VS Code in the browser",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"browser": "./dist/web/extension.js",
	"scripts": {
		"compile-web": "webpack",
		"package-web": "webpack --mode production --devtool hidden-source-map",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run package-web",
		"watch-web": "webpack --watch"
	},
	"contributes": {
		"commands": [
			{
				"command": "helloworld-web-sample.helloWorld",
				"title": "Hello World"
			}
		]
	},
	"activationEvents": [],
	"devDependencies": {
		"@types/mocha": "^9.0.0",
		"@types/vscode": "^1.73.0",
		"@types/webpack-env": "^1.16.2",
		"@vscode/test-web": "^0.0.22",
		"assert": "^2.0.0",
		"mocha": "^9.2.0",
		"process": "^0.11.10",
		"ts-loader": "^9.2.5",
		"typescript": "^5.4.2",
		"typescript-esbuild": "latest",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
