{
	"name": "codelens-sample",
	"displayName": "CodeLens Sample",
	"version": "0.0.1",
	"private": true,
	"description": "Samples for VS Code's CodeLens API",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "ms-vscode",
	"main": "./out/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"title": "Enable CodeLens",
				"command": "codelens-sample.enableCodeLens",
				"category": "CodeLens Sample"
			},
			{
				"title": "Disable Codelens",
				"command": "codelens-sample.disableCodeLens",
				"category": "CodeLens Sample"
			}
		],
		"configuration": {
			"properties": {
				"codelens-sample.enableCodeLens": {
					"type": "boolean",
					"default": true
				}
			}
		}
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@types/node": "20.10.6",
		"@types/vscode": "1.85.0",
		"typescript": "5.3.3"
	}
}
