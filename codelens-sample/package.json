{
	"name": "codelens-sample",
	"displayName": "CodeLens Sample",
	"version": "0.0.1",
	"private": false,
	"description": "Samples for VS Code's CodeLens API",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"category": "CodeLens Sample",
				"command": "codelens-sample.enableCodeLens",
				"title": "Enable CodeLens"
			},
			{
				"category": "CodeLens Sample",
				"command": "codelens-sample.disableCodeLens",
				"title": "Disable Codelens"
			}
		],
		"configuration": {
			"properties": {
				"codelens-sample.enableCodeLens": {
					"default": true,
					"type": "boolean"
				}
			}
		}
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "^18"
	},
	"publishConfig": {
		"access": "public"
	}
}
