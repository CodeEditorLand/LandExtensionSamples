{
  "name": "codelens-sample",
  "displayName": "CodeLens Sample",
  "version": "0.0.1",
  "private": "false",
  "description": "Samples for VS Code's CodeLens API",
  "categories": [
    "Other"
  ],
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "publisher": "playform",
  "type": "module",
  "main": "./out/extension",
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "commands": [
      {
        "title": "Enable CodeLens",
        "command": "codelens-sample.enableCodeLens",
        "category": "CodeLens Sample"
      },
      {
        "title": "Disable Codelens",
        "command": "codelens-sample.disableCodeLens",
        "category": "CodeLens Sample"
      }
    ],
    "configuration": {
      "properties": {
        "codelens-sample.enableCodeLens": {
          "type": "boolean",
          "default": true
        }
      }
    }
  },
  "activationEvents": [
    "*"
  ],
  "devDependencies": {
    "@types/node": "20.8.9",
    "@types/vscode": "1.83.1",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.52.0",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1"
  },
  "engines": {
    "vscode": "1.73.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
