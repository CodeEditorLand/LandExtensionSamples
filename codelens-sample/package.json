{
	"name": "codelens-sample",
	"displayName": "CodeLens Sample",
	"version": "0.0.1",
	"private": true,
	"description": "Samples for VS Code's CodeLens API",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "ms-vscode",
	"main": "./out/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"title": "Enable CodeLens",
				"command": "codelens-sample.enableCodeLens",
				"category": "CodeLens Sample"
			},
			{
				"title": "Disable Codelens",
				"command": "codelens-sample.disableCodeLens",
				"category": "CodeLens Sample"
			}
		],
		"configuration": {
			"properties": {
				"codelens-sample.enableCodeLens": {
					"type": "boolean",
					"default": true
				}
			}
		}
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@types/node": "20.9.1",
		"@types/vscode": "1.84.1",
		"@typescript-eslint/eslint-plugin": "6.11.0",
		"@typescript-eslint/parser": "6.11.0",
		"eslint": "8.53.0",
		"typescript": "5.2.2"
	},
	"engines": {
		"vscode": "1.73.0"
	}
}
