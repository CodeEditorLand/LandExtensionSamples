{
	"name": "codelens-sample",
	"displayName": "CodeLens Sample",
	"version": "0.0.1",
	"private": false,
	"description": "Samples for VS Code's CodeLens API",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"category": "CodeLens Sample",
				"command": "codelens-sample.enableCodeLens",
				"title": "Enable CodeLens"
			},
			{
				"category": "CodeLens Sample",
				"command": "codelens-sample.disableCodeLens",
				"title": "Disable Codelens"
			}
		],
		"configuration": {
			"properties": {
				"codelens-sample.enableCodeLens": {
					"default": true,
					"type": "boolean"
				}
			}
		}
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"typescript": "^5.4.2",
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	}
}
