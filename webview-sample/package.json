{
	"name": "cat-coding",
	"version": "0.0.1",
	"private": false,
	"description": "Cat Coding - A Webview API Sample",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -w -p ./"
	},
	"contributes": {
		"commands": [
			{
				"category": "Cat Coding",
				"command": "catCoding.start",
				"title": "Start cat coding session"
			},
			{
				"category": "Cat Coding",
				"command": "catCoding.doRefactor",
				"title": "Do some refactoring"
			}
		]
	},
	"activationEvents": [
		"onWebviewPanel:catCoding"
	],
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@types/vscode-webview": "^1.57.0",
		"typescript": "^5.4.2",
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	}
}
