{
	"name": "notebook-renderer-react-sample",
	"displayName": "notebook-renderer-react-sample",
	"version": "0.0.1",
	"private": false,
	"description": "Example of a Notebook renderer (visualizer) for GitHub Issues Notebook built with React.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"compile": "npm run compile:extension && npm run compile:client",
		"compile:client": "webpack --mode production",
		"compile:extension": "tsc -b",
		"dev": "concurrently -r npm:watch:extension npm:watch:client",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile && node out/test/checkNoTestProvider.js",
		"watch": "concurrently -r \"npm:watch:*\"",
		"watch:client": "webpack --mode development --watch",
		"watch:extension": "tsc -b --watch"
	},
	"contributes": {
		"notebookRenderer": [
			{
				"displayName": "GitHub Issues React-based Renderer",
				"entrypoint": "./out/client/index.js",
				"id": "notebook-renderer-react-github-issues",
				"mimeTypes": [
					"x-application/github-issues"
				]
			}
		]
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/node": "^18.15.5",
		"@types/react": "^17.0.15",
		"@types/react-dom": "^17.0.9",
		"@types/vscode": "^1.58.0",
		"@types/vscode-notebook-renderer": "^1.57.8",
		"@types/webpack-env": "^1.16.0",
		"concurrently": "^5.3.0",
		"css-loader": "^4.2.0",
		"glob": "^7.1.7",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"style-loader": "^1.2.1",
		"ts-loader": "^9.4.2",
		"typescript": "^5.4.2",
		"typescript-esbuild": "latest",
		"vscode-notebook-error-overlay": "^1.0.1",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
