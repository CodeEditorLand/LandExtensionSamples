{
	"name": "notebook-renderer-react-sample",
	"displayName": "notebook-renderer-react-sample",
	"version": "0.0.1",
	"private": false,
	"description": "Example of a Notebook renderer (visualizer) for GitHub Issues Notebook built with React.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "npm run compile:extension && npm run compile:client",
		"compile:client": "webpack --mode production",
		"compile:extension": "tsc -b",
		"dev": "concurrently -r npm:watch:extension npm:watch:client",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"watch:client": "webpack --mode development --watch",
		"watch:extension": "tsc -b --watch"
	},
	"contributes": {
		"notebookRenderer": [
			{
				"displayName": "GitHub Issues React-based Renderer",
				"entrypoint": "./out/client/index.js",
				"id": "notebook-renderer-react-github-issues",
				"mimeTypes": [
					"x-application/github-issues"
				]
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "^7.1.3",
		"@types/node": "^18",
		"concurrently": "^5.3.0",
		"css-loader": "^4.2.0",
		"glob": "^7.1.7",
		"style-loader": "^1.2.1",
		"ts-loader": "^9.4.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
