{
	"name": "chat-sample",
	"displayName": "Copilot Chat Sample",
	"version": "0.0.1",
	"private": false,
	"description": "Sample chat extension, a trusty cat tutor that will can teach you computer science topics.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"chatParticipants": [
			{
				"commands": [
					{
						"description": "Pick at random a computer science concept then explain it in purfect way of a cat",
						"name": "teach"
					},
					{
						"description": "Do whatever you want, you are a cat after all",
						"name": "play"
					}
				],
				"description": "Meow! What can I teach you?",
				"id": "chat-sample.cat",
				"isSticky": true,
				"name": "cat"
			}
		],
		"commands": [
			{
				"command": "cat.namesInEditor",
				"title": "Use Cat Names in Editor"
			}
		]
	},
	"activationEvents": [],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "^20.5.9"
	},
	"extensionDependencies": [
		"github.copilot-chat"
	],
	"publishConfig": {
		"access": "public"
	},
	"enabledApiProposals": [
		"chatParticipant",
		"chatVariableResolver",
		"languageModels"
	]
}
