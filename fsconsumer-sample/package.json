{
	"name": "vscode-memfs",
	"displayName": "FileSystem Consumer Sample",
	"version": "0.0.1",
	"private": false,
	"description": "Showcase for using the workspace.fs.-API which allows reading and writing files and folder",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/src/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"category": "FS",
				"command": "fs/openJS",
				"title": "Open JavaScript-file for TypeScript-file..."
			},
			{
				"category": "FS",
				"command": "fs/sumSizes",
				"title": "Show folder information for current file"
			},
			{
				"category": "FS",
				"command": "fs/readWriteFile",
				"title": "Read and Write file"
			}
		]
	},
	"activationEvents": [],
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"typescript": "^5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
