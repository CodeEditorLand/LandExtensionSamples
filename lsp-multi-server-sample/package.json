{
	"name": "lsp-sample-multi-server",
	"version": "0.0.1",
	"private": false,
	"description": "A multi server LSP example",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üõ¨‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./client/out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -b",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"lspMultiServerSample.enable": {
					"default": true,
					"description": "Controls the enablement.",
					"scope": "resource",
					"type": "boolean"
				},
				"lspMultiServerSample.options": {
					"default": {},
					"description": "Additional options.",
					"scope": "resource",
					"type": "object"
				},
				"lspMultiServerSample.trace.server": {
					"default": "off",
					"description": "Traces the communication between VSCode and the language server.",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"scope": "window",
					"type": "string"
				}
			},
			"title": "Multi LSP configuration",
			"type": "object"
		}
	},
	"activationEvents": [
		"onLanguage:plaintext"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.10"
	},
	"publishConfig": {
		"access": "public"
	}
}
