{
	"name": "references-plusplus",
	"displayName": "References++",
	"version": "0.0.1",
	"private": false,
	"description": "Show the results of 'Find References' as formatted text in an editor",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "editor.printReferences",
				"title": "Show All References"
			}
		],
		"grammars": [
			{
				"language": "locations",
				"path": "./locations-syntax.json",
				"scopeName": "source.locations"
			}
		],
		"languages": [
			{
				"aliases": [
					"Locations"
				],
				"extensions": [
					".locations"
				],
				"id": "locations"
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "editor.printReferences",
					"group": "navigation@1.31",
					"when": "editorHasReferenceProvider"
				}
			]
		}
	},
	"activationEvents": [],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20"
	},
	"publishConfig": {
		"access": "public"
	}
}
