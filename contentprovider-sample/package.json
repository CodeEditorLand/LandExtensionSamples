{
  "name": "references-plusplus",
  "displayName": "References++",
  "version": "0.0.1",
  "private": "false",
  "description": "Show the results of 'Find References' as formatted text in an editor",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "publisher": "playform",
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "contributes": {
    "commands": [
      {
        "command": "editor.printReferences",
        "title": "Show All References"
      }
    ],
    "grammars": [
      {
        "language": "locations",
        "path": "./locations-syntax.json",
        "scopeName": "source.locations"
      }
    ],
    "languages": [
      {
        "id": "locations",
        "aliases": [
          "Locations"
        ],
        "extensions": [
          ".locations"
        ]
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "editor.printReferences",
          "when": "editorHasReferenceProvider",
          "group": "navigation@1.31"
        }
      ]
    }
  },
  "activationEvents": [],
  "devDependencies": {
    "@types/node": "20.9.2",
    "@types/vscode": "1.84.1",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  }
}
