{
	"name": "references-plusplus",
	"displayName": "References++",
	"version": "0.0.5",
	"private": true,
	"description": "Show the results of 'Find References' as formatted text in an editor",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "vscode-samples",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "editor.printReferences",
				"title": "Show All References"
			}
		],
		"grammars": [
			{
				"language": "locations",
				"path": "./locations-syntax.json",
				"scopeName": "source.locations"
			}
		],
		"languages": [
			{
				"id": "locations",
				"aliases": [
					"Locations"
				],
				"extensions": [
					".locations"
				]
			}
		],
		"menus": {
			"editor/context": [
				{
					"command": "editor.printReferences",
					"when": "editorHasReferenceProvider",
					"group": "navigation@1.31"
				}
			]
		}
	},
	"activationEvents": [],
	"devDependencies": {
		"@types/node": "20.10.5",
		"@types/vscode": "1.85.0",
		"typescript": "5.3.3"
	}
}
