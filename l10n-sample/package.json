{
	"activationEvents": [],
	"contributes": {
		"commands": [
			{
				"command": "extension.sayHello",
				"title": "%extension.sayHello.title%"
			},
			{
				"command": "extension.sayBye",
				"title": "%extension.sayBye.title%"
			}
		]
	},
	"dependencies": {
		"@vscode/l10n": "^0.0.10"
	},
	"description": "Sample that shows how to localize an extension",
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.73.0",
		"@vscode/l10n-dev": "^0.0.18",
		"@vscode/test-electron": "^2.3.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"displayName": "l10n-sample",
	"l10n": "./l10n",
	"main": "./out/extension.js",
	"name": "l10n-sample",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"type": "module"
}
