{
	"name": "vscode-authenticationprovider-sample",
	"displayName": "AuthenticationProvider API Sample",
	"version": "0.0.1",
	"private": false,
	"description": "AuthenticationProvider API Sample",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"authentication": [
			{
				"id": "azuredevopspat",
				"label": "Azure DevOps PAT"
			}
		],
		"commands": [
			{
				"command": "vscode-authenticationprovider-sample.login",
				"title": "Login with Azure DevOps"
			}
		]
	},
	"activationEvents": [],
	"dependencies": {
		"isomorphic-fetch": "^3.0.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/isomorphic-fetch": "^0.0.35",
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.60.0",
		"glob": "^7.1.7",
		"typescript": "^5.4.2",
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	}
