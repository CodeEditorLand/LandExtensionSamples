{
	"activationEvents": [
		"onCommand:workbench.action.tasks.runTask"
	],
	"author": {
		"email": "Land@PlayForm.Cloud",
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"contributes": {
		"taskDefinitions": [
			{
				"properties": {
					"file": {
						"description": "The Rake file that provides the task. Can be omitted.",
						"type": "string"
					},
					"task": {
						"description": "The Rake task to customize",
						"type": "string"
					}
				},
				"required": [
					"task"
				],
				"type": "rake"
			},
			{
				"properties": {
					"flags": {
						"description": "Additional build flags.",
						"type": "array"
					},
					"flavor": {
						"description": "The build flavor. Should be either '32' or '64'.",
						"type": "string"
					}
				},
				"required": [
					"flavor"
				],
				"type": "custombuildscript"
			}
		]
	},
	"description": "Samples for VSCode's view API",
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20"
	},
	"displayName": "Task Provider Samples",
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"keywords": [
		"land"
	],
	"license": "SEE LICENSE IN LICENSE",
	"main": "./out/extension",
	"name": "task-provider-samples",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -b",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"type": "module",
	"version": "0.0.1"
}
