		"test": "vscode-test"
	},
	"devDependencies": {
		"@types/glob": "8.1.0",
		"@types/mocha": "10.0.3",
		"@types/node": "20.8.9",
		"@types/vscode": "1.83.1",
		"@vscode/test-cli": "0.0.4",
		"@vscode/test-electron": "2.3.6",
		"mocha": "10.2.0",
		"typescript": "5.2.2"
	}
}
