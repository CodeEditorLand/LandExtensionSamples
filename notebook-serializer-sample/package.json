{
	"name": "notebook-serializer-sample",
	"displayName": "notebook-serializer-sample",
	"version": "0.0.1",
	"private": false,
	"description": "Notebook using Serializer API sample",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üõ¨‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -b",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "notebook-serializer-sample.createJsonNotebook",
				"title": "Create JSON Notebook"
			}
		],
		"notebooks": [
			{
				"displayName": "Sample Notebook",
				"selector": [
					{
						"filenamePattern": "*.sample-json-notebook"
					}
				],
				"type": "test-notebook-serializer"
			}
		]
	},
	"activationEvents": [
		"onNotebook:test-notebook-serializer",
		"onCommand:notebook-serializer-sample.createJsonNotebook"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.x"
	},
	"publishConfig": {
		"access": "public"
	}
}
