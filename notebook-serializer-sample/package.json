{
	"name": "notebook-serializer-sample",
	"displayName": "notebook-serializer-sample",
	"version": "0.0.1",
	"private": false,
	"description": "Notebook using Serializer API sample",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -b",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -b --watch"
	},
	"contributes": {
		"commands": [
			{
				"command": "notebook-serializer-sample.createJsonNotebook",
				"title": "Create JSON Notebook"
			}
		],
		"notebooks": [
			{
				"displayName": "Sample Notebook",
				"selector": [
					{
						"filenamePattern": "*.sample-json-notebook"
					}
				],
				"type": "test-notebook-serializer"
			}
		]
	},
	"activationEvents": [
		"onNotebook:test-notebook-serializer",
		"onCommand:notebook-serializer-sample.createJsonNotebook"
	],
	"devDependencies": {
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.74.0",
		"mocha": "^10.2.0",
		"typescript": "^5.4.2",
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	}
}
