{
	"name": "welcome-view-content-sample",
	"version": "0.0.1",
	"private": false,
	"description": "Uri Handler Sample",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "welcome-view-content-sample.hello",
				"title": "Hello World!"
			}
		],
		"viewsWelcome": [
			{
				"contents": "You can have paragraphs of text here. You can have [links](https://code.visualstudio.com) to external sources or [internal commands](command:welcome-view-content-sample.hello).\nUse new lines to have new paragraphs.\nPlace a link alone in a paragraph to make it a button\n[Hello](command:welcome-view-content-sample.hello)",
				"view": "workbench.explorer.emptyView"
			}
		]
	},
	"activationEvents": [],
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"@vscode/test-electron": "^2.3.0",
		"glob": "^7.1.6",
		"typescript": "^5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
