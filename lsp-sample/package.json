{
	"name": "lsp-sample",
	"version": "0.0.1",
	"private": false,
	"description": "A language server example",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./client/out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -b",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"languageServerExample.maxNumberOfProblems": {
					"default": 100,
					"description": "Controls the maximum number of problems produced by the server.",
					"scope": "resource",
					"type": "number"
				},
				"languageServerExample.trace.server": {
					"default": "off",
					"description": "Traces the communication between VS Code and the language server.",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"scope": "window",
					"type": "string"
				}
			},
			"title": "Example configuration",
			"type": "object"
		}
	},
	"activationEvents": [
		"onLanguage:plaintext"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.7"
	},
	"publishConfig": {
		"access": "public"
	}
}
