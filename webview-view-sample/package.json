{
	"name": "calico-colors",
	"version": "0.0.1",
	"private": true,
	"description": "Calico Colors - A Webview View API Sample",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "vscode-samples",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -w -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "calicoColors.addColor",
				"category": "Calico Colors",
				"title": "Add Color"
			},
			{
				"command": "calicoColors.clearColors",
				"category": "Calico Colors",
				"title": "Clear Colors",
				"icon": "$(clear-all)"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "calicoColors.clearColors",
					"group": "navigation",
					"when": "view == calicoColors.colorsView"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"type": "webview",
					"id": "calicoColors.colorsView",
					"name": "Calico Colors"
				}
			]
		}
	},
	"activationEvents": [],
	"devDependencies": {
		"@types/vscode": "1.85.0",
		"typescript": "5.3.3"
	},
	"extensionKind": [
		"ui",
		"workspace"
	]
}
