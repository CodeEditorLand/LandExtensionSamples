{
	"name": "calico-colors",
	"version": "0.0.1",
	"private": false,
	"description": "Calico Colors - A Webview View API Sample",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"category": "Calico Colors",
				"command": "calicoColors.addColor",
				"title": "Add Color"
			},
			{
				"category": "Calico Colors",
				"command": "calicoColors.clearColors",
				"icon": "$(clear-all)",
				"title": "Clear Colors"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "calicoColors.clearColors",
					"group": "navigation",
					"when": "view == calicoColors.colorsView"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "calicoColors.colorsView",
					"name": "Calico Colors",
					"type": "webview"
				}
			]
		}
	},
	"activationEvents": [],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6"
	},
	"publishConfig": {
		"access": "public"
	},
	"extensionKind": [
		"ui",
		"workspace"
	]
}
