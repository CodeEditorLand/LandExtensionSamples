{
	"name": "calico-colors",
	"version": "0.0.1",
	"private": false,
	"description": "Calico Colors - A Webview View API Sample",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -w -p ./"
	},
	"contributes": {
		"commands": [
			{
				"category": "Calico Colors",
				"command": "calicoColors.addColor",
				"title": "Add Color"
			},
			{
				"category": "Calico Colors",
				"command": "calicoColors.clearColors",
				"icon": "$(clear-all)",
				"title": "Clear Colors"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "calicoColors.clearColors",
					"group": "navigation",
					"when": "view == calicoColors.colorsView"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "calicoColors.colorsView",
					"name": "Calico Colors",
					"type": "webview"
				}
			]
		}
	},
	"activationEvents": [],
	"devDependencies": {
		"@types/vscode": "^1.73.0",
		"typescript": "^5.4.2",
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	},
	"extensionKind": [
		"ui",
		"workspace"
	]
}
