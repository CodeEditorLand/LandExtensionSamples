{
	"activationEvents": [],
	"contributes": {
		"commands": [
			{
				"category": "Calico Colors",
				"command": "calicoColors.addColor",
				"title": "Add Color"
			},
			{
				"category": "Calico Colors",
				"command": "calicoColors.clearColors",
				"icon": "$(clear-all)",
				"title": "Clear Colors"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "calicoColors.clearColors",
					"group": "navigation",
					"when": "view == calicoColors.colorsView"
				}
			]
		},
		"views": {
			"explorer": [
				{
					"id": "calicoColors.colorsView",
					"name": "Calico Colors",
					"type": "webview"
				}
			]
		}
	},
	"description": "Calico Colors - A Webview View API Sample",
	"devDependencies": {},
	"extensionKind": [
		"ui",
		"workspace"
	],
	"main": "./out/extension.js",
	"name": "calico-colors",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"watch": "tsc -w -p ./"
	},
	"devDependencies": {
		"@types/vscode": "^1.73.0",
		"@typescript-eslint/eslint-plugin": "^7.14.0",
		"@typescript-eslint/parser": "^7.14.0",
		"eslint": "^8.32.0",
		"typescript": "^5.6.2"
	}
}
