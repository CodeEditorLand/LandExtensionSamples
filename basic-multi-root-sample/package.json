{
	"name": "basic-multi-root-sample",
	"displayName": "Basic Multi Root Sample",
	"version": "0.0.1",
	"private": true,
	"description": "Samples for VSCode's multi root API",
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "MIT",
	"publisher": "vscode-samples",
	"main": "./out/src/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Basic Multi Root Sample",
			"properties": {
				"multiRootSample.statusColor": {
					"type": [
						"string"
					],
					"default": "#FFFFFF",
					"description": "Color to use for the status bar item. Can be set per workspace folder.",
					"scope": "resource"
				}
			}
		}
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@types/node": "20.10.6",
		"@types/vscode": "1.85.0",
		"typescript": "5.3.3"
	}
}
