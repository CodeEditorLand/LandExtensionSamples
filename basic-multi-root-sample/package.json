{
	"name": "basic-multi-root-sample",
	"displayName": "Basic Multi Root Sample",
	"version": "0.0.1",
	"private": false,
	"description": "Samples for VSCode's multi root API",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/src/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"configuration": {
			"properties": {
				"multiRootSample.statusColor": {
					"default": "#FFFFFF",
					"description": "Color to use for the status bar item. Can be set per workspace folder.",
					"scope": "resource",
					"type": [
						"string"
					]
				}
			},
			"title": "Basic Multi Root Sample",
			"type": "object"
		}
	},
	"activationEvents": [
		"*"
	],
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"typescript": "^5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
