{
  "name": "github-authentication-sample",
  "displayName": "github-authentication-sample",
  "description": "Example of using the GitHub Authentication Provider",
  "version": "0.0.1",
  "publisher": "vscode-samples",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.getGitHubUser",
        "title": "Get GitHub User",
        "category": "GitHub Authentication Sample"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.18.11",
    "@types/vscode": "1.74.0",
    "typescript": "5.3.2"
  },
  "dependencies": {
    "@octokit/rest": "18.0.0"
  }
}
