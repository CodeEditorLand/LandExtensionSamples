{
  "name": "jupyter-server-provider-sample",
  "displayName": "Jupyter Server Provider Sample",
  "version": "0.0.1",
  "description": "Jupyter Notebook Kernel Picker with custom servers using the Jupyter API sample",
  "categories": [
    "Other"
  ],
  "publisher": "playform",
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -b",
    "lint": "eslint src --ext ts",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -b --watch",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "activationEvents": [
    "onNotebook:jupyter-notebook",
    "onNotebook:interactive"
  ],
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "1.84.1",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@vscode/jupyter-extension": "1.0.0",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1"
  },
  "extensionDependencies": [
    "ms-toolsai.jupyter"
  ],
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  }
