{
  "name": "chat-agent-sample",
  "publisher": "playform",
  "displayName": "Copilot Chat Sample Agent",
  "description": "Sample chat agent extension, a trusty cat that will help you with your code.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "version": "0.0.1",
  "extensionDependencies": [
    "github.copilot-chat"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "enabledApiProposals": [
    "chatAgents2",
    "chatRequestAccess"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "devDependencies": {
    "@types/node": "20.9.4",
    "@types/vscode": "1.84.2",
    "eslint": "8.54.0",
    "tslint": "6.1.3",
    "typescript": "5.3.2",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "license": "MIT",
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  }
}
