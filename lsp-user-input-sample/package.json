{
	"name": "vscode-codeaction-ui-sample",
	"displayName": "Code Action UI Sample",
	"version": "0.0.1",
	"private": false,
	"description": "Sample to demonstrate code actions with UI",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./client/out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"clean": "rimraf client/out && rimraf server/out",
		"compile": "tsc -b",
		"compile:client": "tsc -b ./client/tsconfig.json",
		"compile:server": "tsc -b ./server/tsconfig.json",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"webpack": "npm run clean && webpack --mode production --config ./client/webpack.config.js && webpack --mode production --config ./server/webpack.config.js",
		"webpack:dev": "npm run clean && webpack --mode none --config ./client/webpack.config.js && webpack --mode none --config ./server/webpack.config.js"
	},
	"activationEvents": [
		"onLanguage:plaintext"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "^18.11.9",
		"merge-options": "^3.0.4",
		"rimraf": "^5.0.5",
		"ts-loader": "^9.5.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
