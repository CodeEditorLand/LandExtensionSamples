{
	"activationEvents": [
		"onStartupFinished"
	],
	"browser": "./out/extension.js",
	"contributes": {
		"configuration": {
			"id": "tabsSample",
			"properties": {
				"tabs.maxInactiveTime": {
					"default": 30,
					"description": "The number of seconds before closing inactive tabs",
					"type": "number"
				}
			},
			"title": "Tabs API Sample"
		}
	},
	"description": "Sample showing how to use the tabs api",
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.67.0",
		"typescript": "^5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"displayName": "tabs-api-sample",
	"main": "./out/extension.js",
	"name": "tabs-api-sample",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"type": "module"
}
