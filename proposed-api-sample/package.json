{
  "name": "proposed-api-sample",
  "displayName": "proposed-api-sample",
  "version": "0.0.1",
  "private": "false",
  "description": "Sample showing how to use Proposed API",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "publisher": "playform",
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "download-api": "dts dev",
    "postdownload-api": "dts main",
    "postinstall": "npm run download-api",
    "lint": "eslint \"src/**/*.ts\"",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "activationEvents": [],
  "devDependencies": {
    "@types/node": "20.9.2",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@vscode/dts": "0.4.0",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1"
  },
  "enabledApiProposals": [],
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
