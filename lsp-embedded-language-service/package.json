{
	"name": "lsp-embedded-language-service-sample",
	"version": "0.0.1",
	"private": false,
	"description": "A language server example using language services to support embedded languages",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./client/out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -b",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"grammars": [
			{
				"language": "html1",
				"path": "./syntaxes/html1.tmLanguage.json",
				"scopeName": "text.html1.basic"
			}
		],
		"languages": [
			{
				"extensions": [
					".html1"
				],
				"id": "html1"
			}
		]
	},
	"activationEvents": [],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.7"
	},
	"publishConfig": {
		"access": "public"
	}
}
