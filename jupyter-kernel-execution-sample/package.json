{
	"name": "jupyter-kernel-execution-sample",
	"displayName": "Jupyter Kernel Execution Sample",
	"version": "0.0.1",
	"private": false,
	"description": "Sample extension using Jupyter API to execute code against the Python Kernel",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"compile": "tsc -b",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -b --watch"
	},
	"contributes": {
		"commands": [
			{
				"category": "Jupyter Kernel API",
				"command": "jupyterKernelExecution.listKernels",
				"title": "Execute code against a Python Kernel"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "jupyterKernelExecution.listKernels",
					"title": "Execute code against a Python Kernel"
				}
			]
		}
	},
	"activationEvents": [
		"onNotebook:jupyter-notebook",
		"onNotebook:interactive"
	],
	"devDependencies": {
		"@types/node": "14.x",
		"@types/vscode": "^1.82.0",
		"@vscode/jupyter-extension": "^1.1.0",
		"typescript": "^5.2.2",
		"typescript-esbuild": "0.4.5"
	},
	"extensionDependencies": [
		"ms-toolsai.jupyter"
	],
	"publishConfig": {
		"access": "public"
	}
}
