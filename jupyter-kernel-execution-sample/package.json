{
	"name": "jupyter-kernel-execution-sample",
	"displayName": "Jupyter Kernel Execution Sample",
	"version": "0.0.1",
	"private": false,
	"description": "Sample extension using Jupyter API to execute code against the Python Kernel",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -b",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"category": "Jupyter Kernel API",
				"command": "jupyterKernelExecution.listKernels",
				"title": "Execute code against a Python Kernel"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "jupyterKernelExecution.listKernels",
					"title": "Execute code against a Python Kernel"
				}
			]
		}
	},
	"activationEvents": [
		"onNotebook:jupyter-notebook",
		"onNotebook:interactive"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.x"
	},
	"extensionDependencies": [
		"ms-toolsai.jupyter"
	],
	"publishConfig": {
		"access": "public"
	}
}
