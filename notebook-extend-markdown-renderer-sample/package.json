{
	"name": "notebook-extend-markdown-renderer-sample",
	"displayName": "notebook-extend-markdown-renderer-sample",
	"version": "0.0.1",
	"private": false,
	"description": "Example of a Notebook renderer that extends VS Code's built-in notebook markdown rendering with :emoji: support.",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandExtensionSamples/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandExtensionSamples.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"test-compile": "tsc -p ./",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch"
	},
	"contributes": {
		"notebookRenderer": [
			{
				"displayName": "Markdown it Emoji renderer",
				"entrypoint": {
					"extends": "vscode.markdown-it-renderer",
					"path": "./out/emoji.js"
				},
				"id": "vscode-samples.markdown-it.emoji-extension"
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/markdown-it": "14.0.1",
		"markdown-it-emoji": "3.0.0",
		"ts-loader": "9.5.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
