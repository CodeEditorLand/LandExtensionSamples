{
	"name": "vscode-memfs",
	"displayName": "MemFS - a file system provider sample",
	"version": "0.0.1",
	"private": false,
	"description": "Showcase for the file system provider API, also useful for testing again document that are not on disk.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/src/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"category": "MemFS",
				"command": "memfs.workspaceInit",
				"title": "Setup Workspace"
			},
			{
				"category": "MemFS",
				"command": "memfs.init",
				"title": "Create Files"
			},
			{
				"category": "MemFS",
				"command": "memfs.reset",
				"title": "Delete Files"
			},
			{
				"category": "MemFS",
				"command": "memfs.deleteFile",
				"title": "Delete \"file.txt\""
			},
			{
				"category": "MemFS",
				"command": "memfs.addFile",
				"title": "Add \"file.txt\""
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "memfs.init",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.reset",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.deleteFile",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.addFile",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.workspaceInit",
					"when": "workbenchState != workspace"
				}
			]
		}
	},
	"activationEvents": [
		"onFileSystem:memfs"
	],
	"devDependencies": {
		"@types/node": "^16.18.34",
		"@types/vscode": "^1.73.0",
		"typescript": "^5.4.2",
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
