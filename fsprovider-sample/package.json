{
  "name": "vscode-memfs",
  "displayName": "MemFS - a file system provider sample",
  "version": "0.0.1",
  "private": "false",
  "description": "Showcase for the file system provider API, also useful for testing again document that are not on disk.",
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "publisher": "playform",
  "main": "./out/src/extension",
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "contributes": {
    "commands": [
      {
        "command": "memfs.workspaceInit",
        "title": "Setup Workspace",
        "category": "MemFS"
      },
      {
        "command": "memfs.init",
        "title": "Create Files",
        "category": "MemFS"
      },
      {
        "command": "memfs.reset",
        "title": "Delete Files",
        "category": "MemFS"
      },
      {
        "command": "memfs.deleteFile",
        "title": "Delete \"file.txt\"",
        "category": "MemFS"
      },
      {
        "command": "memfs.addFile",
        "title": "Add \"file.txt\"",
        "category": "MemFS"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "memfs.init",
          "when": "workbenchState == workspace"
        },
        {
          "command": "memfs.reset",
          "when": "workbenchState == workspace"
        },
        {
          "command": "memfs.deleteFile",
          "when": "workbenchState == workspace"
        },
        {
          "command": "memfs.addFile",
          "when": "workbenchState == workspace"
        },
        {
          "command": "memfs.workspaceInit",
          "when": "workbenchState != workspace"
        }
      ]
    }
  },
  "activationEvents": [
    "onFileSystem:memfs"
  ],
  "devDependencies": {
    "@types/node": "20.9.2",
    "@types/vscode": "1.84.1",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "typescript": "5.2.2",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  }
}
