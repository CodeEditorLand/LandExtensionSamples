{
	"name": "vscode-memfs",
	"displayName": "MemFS - a file system provider sample",
	"version": "0.0.3",
	"private": true,
	"description": "Showcase for the file system provider API, also useful for testing again document that are not on disk.",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Microsoft/vscode-extension-samples"
	},
	"license": "SEE LICENSE IN LICENSE",
	"publisher": "vscode-samples",
	"main": "./out/src/extension",
	"scripts": {
		"compile": "tsc -p ./",
		"lint": "eslint \"src/**/*.ts\"",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"contributes": {
		"commands": [
			{
				"command": "memfs.workspaceInit",
				"title": "Setup Workspace",
				"category": "MemFS"
			},
			{
				"command": "memfs.init",
				"title": "Create Files",
				"category": "MemFS"
			},
			{
				"command": "memfs.reset",
				"title": "Delete Files",
				"category": "MemFS"
			},
			{
				"command": "memfs.deleteFile",
				"title": "Delete \"file.txt\"",
				"category": "MemFS"
			},
			{
				"command": "memfs.addFile",
				"title": "Add \"file.txt\"",
				"category": "MemFS"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "memfs.init",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.reset",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.deleteFile",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.addFile",
					"when": "workbenchState == workspace"
				},
				{
					"command": "memfs.workspaceInit",
					"when": "workbenchState != workspace"
				}
			]
		}
	},
	"activationEvents": [
		"onFileSystem:memfs"
	],
	"devDependencies": {
		"@types/node": "20.10.3",
		"@types/vscode": "1.84.2",
		"@typescript-eslint/eslint-plugin": "6.13.1",
		"@typescript-eslint/parser": "6.13.1",
		"eslint": "8.55.0",
		"typescript": "5.3.2"
	},
	"engines": {
		"vscode": "1.74.0"
	}
}
