{
	"name": "notebook-renderer-sample",
	"displayName": "notebook-renderer-sample",
	"version": "0.0.1",
	"private": false,
	"description": "Example of a Notebook renderer (visualizer) for GitHub Issues Notebook that doesn't use any frontend frameworks.",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"test-compile": "tsc -p ./",
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch"
	},
	"contributes": {
		"notebookRenderer": [
			{
				"displayName": "GitHub Issues Plain Renderer",
				"entrypoint": "./out/index.js",
				"id": "notebook-renderer-github-issues",
				"mimeTypes": [
					"x-application/github-issues"
				]
			}
		]
	},
	"devDependencies": {
		"@types/vscode-notebook-renderer": "^1.57.8",
		"@types/webpack-env": "^1.17.0",
		"css-loader": "^4.2.0",
		"style-loader": "^1.2.1",
		"ts-loader": "^9.2.2",
		"typescript": "^5.4.2",
		"typescript-esbuild": "0.4.5",
		"vscode-notebook-error-overlay": "^1.0.1",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
